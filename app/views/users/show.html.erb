<h1><%= @user.name %></h1>

<% @user.work_days.each do |f| %>
  <h1><%= f.work_date.strftime('%a %b %d %Y') %></h1>

  <% f.punches.each do |x| %>
    In: <%= x.in.strftime('%I:%M:%S %p') %><br>
    Out: <%= x.out.strftime('%I:%M:%S %p') %><br>
    Total: <%= ((x.out - x.in) / 3600).round(2) %> hours
  <% end %>
<% end %>
